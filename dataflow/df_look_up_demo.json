{
	"name": "df_look_up_demo",
	"properties": {
		"folder": {
			"name": "LookupDemo"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_blob_employe_table",
						"type": "DatasetReference"
					},
					"name": "SrEmployeTable"
				},
				{
					"dataset": {
						"referenceName": "ds_blob_department_table",
						"type": "DatasetReference"
					},
					"name": "SrDepartmentTable"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          Name as string,",
				"          DepartmentID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrEmployeTable",
				"source(output(",
				"          DepartmentID as string,",
				"          DepartmentName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrDepartmentTable"
			]
		}
	}
}